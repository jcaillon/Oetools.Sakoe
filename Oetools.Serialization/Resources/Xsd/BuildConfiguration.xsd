<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="BuildConfiguration" nillable="true" type="XmlOeBuildConfiguration" />
  <xs:complexType name="XmlOeBuildConfiguration">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="OutputDirectory" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="ReportFilePath" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="BuildHistoryOutputFilePath" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="BuildHistoryInputFilePath" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="CompilationOptions" type="XmlOeCompilationOptions" />
      <xs:element minOccurs="0" maxOccurs="1" name="BuildOptions" type="XmlOeBuildOptions" />
      <xs:element minOccurs="0" maxOccurs="1" name="SourcePathFilters" type="ArrayOfXmlOeSourceFilter" />
      <xs:element minOccurs="0" maxOccurs="1" name="TaskVariables" type="ArrayOfXmlOeTaskVariable" />
      <xs:element minOccurs="0" maxOccurs="1" name="SourceCompilationTasks" type="ArrayOfChoice1" />
      <xs:element minOccurs="0" maxOccurs="1" name="SourceFilesTasks" type="ArrayOfChoice2" />
      <xs:element minOccurs="0" maxOccurs="1" name="OutputDeploymentTasks" type="ArrayOfXmlOeDeploymentStep" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="XmlOeCompilationOptions">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="CompileWithDebugList" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileWithXref" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileWithListing" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileUseXmlXref" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileForceUsageOfTemporaryDirectory" type="xs:boolean" />
      <xs:element minOccurs="0" maxOccurs="1" name="CompilableFilePattern" type="xs:string" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileForceSingleProcess" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileNumberProcessPerCore" type="xs:unsignedByte" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="XmlOeBuildOptions">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="ArchivesCompressionLevel" type="XmlOeCompressionLevel" />
      <xs:element minOccurs="1" maxOccurs="1" name="StoreSourceMd5" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="CreatePackageInTempDir" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="XmlOeCompressionLevel">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="Max" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ArrayOfXmlOeSourceFilter">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Filter" nillable="true" type="XmlOeSourceFilter" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="XmlOeSourceFilter">
    <xs:attribute name="Exclude" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="ArrayOfXmlOeTaskVariable">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Variable" nillable="true" type="XmlOeTaskVariable" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="XmlOeTaskVariable">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="Name" type="xs:string" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ArrayOfChoice1">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element minOccurs="1" maxOccurs="1" name="Execute" nillable="true" type="XmlOeTaskExec" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileInCab" nillable="true" type="XmlOeTaskCab" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileInZip" nillable="true" type="XmlOeTaskZip" />
      <xs:element minOccurs="1" maxOccurs="1" name="CompileUploadFtp" nillable="true" type="XmlOeTaskFtp" />
